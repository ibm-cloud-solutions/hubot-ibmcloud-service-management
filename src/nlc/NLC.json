{   "name": "IBMcloudServiceManagment",
    "version": "1.0",
    "classes": [
        {
            "class": "bluemix.service.bind",
            "description": "Bind Bluemix service to an application",
            "emittarget": "bluemix.service.bind",
            "texts": [
					"I would like to bind my service",
					"Bind my Bluemix service",
					"Bind my service to an application"
            ]
        },
		  {
            "class": "bluemix.service.create",
            "description": "Create a Bluemix service",
            "emittarget": "bluemix.service.create",
            "texts": [
                     "Service create",
					 "Create my service",
					 "Create a Bluemix service",
					 "Build service"
            ],
				"parameters" : [
				  {
						"name": "servicename",
						"type": "entity",
						"prompt": "OK. What is the name of the service to create?"
				  }
				]
        },
		  {
            "class": "bluemix.service.help",
            "description": "Help with Bluemix services",
            "emittarget": "bluemix.service.help",
            "texts": [
					"Help service",
					"Commands for service",
					"Help with Bluemix services",
					"What are the service commands"
            ]
        },
		  {
            "class": "bluemix.service.list",
            "description": "List all Bluemix services",
            "emittarget": "bluemix.service.list",
            "texts": [
					"List all Bluemix services",
					"Show Bluemix services",
					"Can I have a list of all the Bluemix services"
            ]
        },
		  {
            "class": "bluemix.space.services.list",
            "description": "Show Bluemix services in the current space",
            "emittarget": "bluemix.space.services.list",
            "texts": [
					"Show my services",
					"Show Bluemix services in the current space",
					"Show services in my space",
					"Display my services"
            ]
        },
		  {
            "class": "bluemix.service.remove",
            "description": "Remove a Bluemix service",
            "emittarget": "bluemix.service.remove",
            "texts": [
					"Eliminate my service",
					"I would like to remove my service",
					"Get rid of my service"
            ],
				"parameters" : [
				  {
						"name": "myservicename",
						"type": "entity",
						"prompt": "OK. What is the name of the service to remove?"
				  }
				]
        },
		  {
            "class": "bluemix.service.unbind",
            "description": "Unbind a Bluemix service",
            "emittarget": "bluemix.service.unbind",
            "texts": [
                	"Service unbind",
			"Unhinge my service",
			"Unbind my Bluemix service from application"
            ]
        }
    ],
	"parameter.values" : [
		{
			"name": "servicename",
			"values": []
		},
		{
			"name": "myservicename",
			"values": []
		}
	]
}
